{
  "name": "skip-go",
  "version": "0.0.0",
  "description": "Skip Go",
  "private": true,
  "workspaces": [
    "packages/core",
    "packages/widget",
    "examples/*",
    "vendor/"
  ],
  "scripts": {
    "dev": "yarn run dev:widget & yarn run dev:nextjs & yarn run dev:core",
    "dev:widget": "yarn workspace @skip-go/widget run dev",
    "dev:nextjs": "yarn workspace nextjs run dev",
    "dev:core": "yarn workspace @skip-router/core run watch",
    "build": "yarn workspace @skip-go/widget run build",
    "build:nextjs": "yarn build && yarn workspace nextjs build",
    "build:core": "yarn workspace @skip-router/core build",
    "chains:update": "git submodule init && git submodule update --remote",
    "chains:generate": "yarn workspace @skip-go/widget run chains:generate",
    "chains": "yarn run chains:update && yarn run chains:generate",
    "postinstall": "yarn run chains"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@cosmjs/amino": "0.32.4",
    "@cosmjs/cosmwasm-stargate": "0.32.4",
    "@cosmjs/encoding": "0.32.4",
    "@cosmjs/math": "0.32.4",
    "@cosmjs/proto-signing": "0.32.4",
    "@cosmjs/stargate": "0.32.4",
    "@cosmjs/tendermint-rpc": "0.32.4",
    "@cosmos-kit/cosmostation-extension": "^2.7.10",
    "@cosmos-kit/keplr-extension": "^2.7.9",
    "@cosmos-kit/leap-extension": "^2.7.10",
    "@cosmos-kit/okxwallet": "2.4.x",
    "@cosmos-kit/react": "2.10.x",
    "@cosmos-kit/station": "2.5.x",
    "@cosmos-kit/vectis": "2.6.x",
    "@graz-sh/types": "^0.0.14",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "cosmjs-types": "0.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "globby": "^14.0.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.2.3",
    "p-map": "^7.0.2",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "rollup": "^2.67.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.0",
    "rollup-plugin-visualizer": "^5.5.4",
    "ts-jest": "^29.1.2",
    "tslib": "^1.9.3",
    "tsup": "^8.0.1",
    "tsx": "^4.12.0",
    "typescript": "5.2.x",
    "typescript-plugin-styled-components": "3.0.0",
    "viem": "^2.7.16",
    "wagmi": "^2.5.7"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@changesets/cli": "^2.24.4"
  }
}
